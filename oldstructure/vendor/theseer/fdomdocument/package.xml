<?xml version="1.0" encoding="UTF-8"?>
<package packagerversion="1.4.10" version="2.0"
         xmlns="http://pear.php.net/dtd/package-2.0"
         xmlns:tasks="http://pear.php.net/dtd/tasks-1.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0
                      http://pear.php.net/dtd/tasks-1.0.xsd
                      http://pear.php.net/dtd/package-2.0
                      http://pear.php.net/dtd/package-2.0.xsd">
    <name>fDOMDocument</name>
    <channel>pear.netpirates.net</channel>
    <summary>An Extension to PHP's standard DOM to add various convenience methods and exceptions by default</summary>
    <description>An Extension to PHP's standard DOM to add various convenience methods and exceptions by default.
    </description>
    <lead>
        <name>Arne Blankerts</name>
        <user>theseer</user>
        <email>arne@blankerts.de</email>
        <active>yes</active>
    </lead>
    <date>2014-02-19</date>
    <version>
        <release>1.5.0</release>
        <api>1.5.0</api>
    </version>
    <stability>
        <release>stable</release>
        <api>stable</api>
    </stability>
    <license>BSD License</license>
    <notes>http://github.com/theseer/fDOMDocument/tree</notes>
    <contents>
        <dir name="/">
            <dir name="TheSeer">
                <dir name="fDOMDocument">
                    <dir name="css">
                        <file name="DollarEqualRule.php" role="php"/>
                        <file name="NotRule.php" role="php"/>
                        <file name="NthChildRule.php" role="php"/>
                        <file name="RegexRule.php" role="php"/>
                        <file name="RuleInterface.php" role="php"/>
                        <file name="Translator.php" role="php"/>
                    </dir>
                    <file name="autoload.php" role="php"/>
                    <file name="fDOMDocumentFragment.php" role="php"/>
                    <file name="fDOMDocument.php" role="php"/>
                    <file name="fDOMElement.php" role="php"/>
                    <file name="fDOMException.php" role="php"/>
                    <file name="fDOMNode.php" role="php"/>
                    <file name="fDOMXPath.php" role="php"/>
                    <file name="XPathQuery.php" role="php"/>
                    <file name="XPathQueryException.php" role="php"/>
                </dir>
            </dir>
            <file baseinstalldir="/" name="README.md" role="doc"/>
            <file baseinstalldir="/" name="LICENSE" role="doc"/>
            <file baseinstalldir="/" name="phpunit.xml.dist" role="test"/>
            <dir name="tests">
                <dir name="_data">
                    <file name="broken.xml" role="test"/>
                    <file name="undefentity.xml" role="test"/>
                    <file name="valid.xml" role="test"/>
                    <file name="selector.xml" role="test"/>
                </dir>
                <file name="fDOMDocumentFragment.test.php" role="test"/>
                <file name="fDOMDocument.test.php" role="test"/>
                <file name="fDOMElement.test.php" role="test"/>
                <file name="fDOMXPath.test.php" role="test"/>
                <file name="XPathQuery.test.php" role="test"/>
                <file name="Translator.test.php" role="test"/>
            </dir>
        </dir>
    </contents>
    <dependencies>
        <required>
            <php>
                <min>5.3.3</min>
            </php>
            <pearinstaller>
                <min>1.9.1</min>
            </pearinstaller>
            <extension>
                <name>libxml</name>
            </extension>
            <extension>
                <name>dom</name>
            </extension>
        </required>
    </dependencies>
    <phprelease/>
</package>
