version: "3.2"
services:
  centreon-web:
    image: "@WEB_IMAGE@"
    container_name: e2e_centreon-web
    ports:
      - 80
  cypress:
    depends_on: 
      - centreon-web
    image: cypress/included:7.6.0
    container_name: e2e_cypress
    user: "@UID@"
    volumes:
      - .:/e2e
    working_dir: /e2e