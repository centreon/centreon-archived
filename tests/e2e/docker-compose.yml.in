version: "3.2"
services:
  centreon-web:
    image: "@WEB_IMAGE@"
    container_name: e2e_centreon-web
    ports:
      - 80
  cypress:
    depends_on: 
      - centreon-web
    image: registry.centreon.com/cypress-included-docker:7.7.0
    container_name: e2e_cypress
    user: "@UID@"
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
      - .:/e2e
    working_dir: /e2e
