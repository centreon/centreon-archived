    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "systemjs": "^3.0.0",
    "systemjs-plugin-css": "^0.1.37"
  },
  "jest": {
    "testResultsProcessor": "jest-junit",
    "testURL": "http://localhost/"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./jest-test-results.xml"
  }
}
