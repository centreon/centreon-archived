{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Submit result to a host or service",
    "type": "object",
    "required": ["status"],
    "properties": {
        "status": {
            "type": "string",
            "enum": [
                "up",
                "down",
                "unreachable",
                "ok",
                "warning",
                "critical",
                "unknown"
            ]
        },
        "performance_data": {
            "type": [
                "string",
                "null"
            ]
        },
        "output": {
            "type": ["string",
                "null"
            ]
        }
    }
}